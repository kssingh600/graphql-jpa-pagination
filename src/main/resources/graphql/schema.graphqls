#Root opeartio Types

type Query{
    allPeople: [Person]
    allPeoplePaged(page: Int, size: Int) : [Person]
}



# Object Types
type Person {
    id: ID!
    firstName: String
    lastName: String
    phoneNumber: String
    email: String
    address: Address
}

type Address{
    id: ID!
    address: String
    city: String
    state: String
    zip: String
}